pipeline{
    agent any

    stages{
        stage('Build'){
            steps{
                sh 'mvn -B -DskipTests clean package'
            }
        }
        stage('Test'){
            steps{
                sh 'mvn test'
            }
            post{
                always{
                    junut 'target/surefire-reports/*.xml'
                }
            }
        }
        stage('Delivery'){
            steps{
                echo "Delivery completed"
            }
        }
    }
}